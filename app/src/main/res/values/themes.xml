<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Mymanager" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <!--        Theme.MaterialComponents.DayNight.DarkActionBar-->
        <item name="colorPrimary">#68A7AD</item>
        <item name="colorPrimaryVariant">@color/skyblue</item>
        <item name="colorOnPrimary">@color/beige</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/skyblue</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Mymanager.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Mymanager.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Mymanager.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
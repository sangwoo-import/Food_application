<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E5CB9F">

    <Button
        android:id="@+id/btn_open"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:backgroundTint="#99C4C8"
        android:text="맞춤 기사"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="#99C4C8"
        android:gravity="center"
        android:text="벌크업"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.178"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/bulk_btn"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_margin="0dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/img"
        android:gravity="center"
        android:scaleType="fitXY"
        android:text="벌크업"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Diet_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.264"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="#99C4C8"
        android:gravity="center"
        android:text="다이어트"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.178"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/Diet_btn"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_margin="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/img_diet"
        android:gravity="center"
        android:scaleType="fitXY"
        android:text="다이어트"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bulk_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.265"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:background="#99C4C8"
        android:gravity="center"
        android:text="고혈압"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/go_btn"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_margin="0dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/go"
        android:gravity="center"
        android:scaleType="fitXY"
        android:text="고혈압"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/dn_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.682"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="#99C4C8"
        android:gravity="center"
        android:text="당뇨병"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/dn_btn"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_margin="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/img_1"
        android:gravity="center"
        android:scaleType="fitXY"
        android:text="당뇨병"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/go_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.682"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>







